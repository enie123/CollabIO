        var Hunt = {lat: 40.441107, lng: -79.9435866};
        var Baker = {lat: 40.441479, lng: -79.945011};
        var Mudge = {lat: 40.446826, lng: -79.942677};
        var Gates = {lat: 40.443477, lng: -79.944647};

        var map = new google.maps.Map(document.getElementById('map'), {
          center: {lat:40.443716, lng: -79.942919},
          zoom: 16
        });

        var marker = new google.maps.Marker({
          position: Hunt, 
          map: map,
          animation: google.maps.Animation.DROP,
          title: 'Hunt Library Collab Session : 4:30'
        });

        var marker = new google.maps.Marker({
          position: Baker, 
          map:map, 
          animation: google.maps.Animation.DROP,
          title: 'Baker 237B Collab Session : 7:30'
        });

        var marker = new google.maps.Marker({
